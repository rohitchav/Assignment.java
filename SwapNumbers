class SwapNumber

{

public static void main(String[] args) {

        int no1=5,no2=4,no3;

        System.out.println("Number Before Swapping(No1)="+no1);

        System.out.println("Number Before Swapping(No2)="+no2);

        no3=no1;

        no1=no2;

        no2=no3;

        System.out.println("Number After Swapping=(No1)"+no1);

        System.out.println("Number After Swapping(No2)="+no2);

    }

}    

    
